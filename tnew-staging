        <script>
            function importCssFile(url) {
            const linkElement = document.createElement('link');
            linkElement.rel = 'stylesheet';
            linkElement.href = url;
            document.head.appendChild(linkElement);
            }

            // FORWARD TO CORRECT KEYWORDS
            const siteFlag = new URLSearchParams(document.location.search).get("site");
            const kidSearchFlag = new URLSearchParams(document.location.search).get("kid");
            const keywordSearchFlag = new URLSearchParams(document.location.search).get("k");

            const urlString = document.location.href;
            const url = new URL(urlString);
            url.search = "";
            const cleanUrl = url.toString();

            if ((cleanUrl == "https://secure.pittsburghkids.org/" || (cleanUrl == "https://secure.pittsburghkids.org/events")) && !kidSearchFlag && !keywordSearchFlag) {
                if (siteFlag=="mlab" || document.cookie.indexOf('site_theme=mlab') != -1) { // if mlab site
                    window.location.replace("https://secure.pittsburghkids.org/?site=mlab&kid=32,38");
                }
                else {
                    window.location.replace("https://secure.pittsburghkids.org/?site=cmp&kid=5,29,30");
                }
            }



            function addStyle(styleString) {
                const style = document.createElement('style');
                style.textContent = styleString;
                document.head.append(style);
            }

            
            function getFirstClassEl(className) {
                classEl = document.getElementsByClassName(className);
                if (classEl.length > 0) {
                    return classEl[0];
                }
                return;
            }

            function addPToEnd(elem, text) {
                elem.appendChild(document.createElement("p"))
                .appendChild(document.createTextNode(text));
            }


            if (document.getElementsByClassName("tn-additional-items-page").length > 0) {
                let allAddOnsHTML = document.getElementsByClassName("tn-prod-list-item__perf-list-item");
                let allAddOnsArr = Array.prototype.slice.call( allAddOnsHTML );


                function connectURLToAddOn(el) {
                    let modalButtonWithIDsList = el.getElementsByClassName("tn-btn-modal-ticket-selector");
                    if (modalButtonWithIDsList.length > 0) {
                        let modalButtonWithIDs = modalButtonWithIDsList[0];
                        let prodSeasonID = modalButtonWithIDs.getAttribute("data-tn-production-season-id");
                        let performanceID = modalButtonWithIDs.getAttribute("data-tn-performance-id");

                        if (!!prodSeasonID && !!performanceID) {
                            let perfURL = "https://secure.pittsburghkids.org/" + prodSeasonID + "/" + performanceID;
                            let perfTitleList = el.getElementsByClassName("tn-performance-title");
                            if (perfTitleList.length > 0) {
                                let perfTitleSpan = perfTitleList[0];
                                let perfTitleLink = document.createElement("a");
                                perfTitleLink.innerText = perfTitleSpan.innerText;
                                perfTitleLink.href = perfURL;
                                perfTitleLink.target="_blank";
                                perfTitleLink.rel="noopener noreferrer";
                                perfTitleSpan.innerHTML = ""; perfTitleSpan.append(perfTitleLink);
                            }
                        }
                    }
                }
                allAddOnsArr.map(el => connectURLToAddOn(el)); 

                function productTypefromHTMLEl(el) {
                    let prodTypeList = el.getElementsByClassName("tn-product-type-name");
                    if (prodTypeList.length > 0) {
                        return prodTypeList[0].innerText;
                        }
                    }

                let overarchingDiv = document.getElementById("tn-heading-production-seasons-list"); //add logic for when null
                overarchingDiv.innerHTML = "";

                function createNewHeaderAndList(prodType, headerName) {
                    let prodOfTypeArr = allAddOnsArr.filter(el => productTypefromHTMLEl(el) == prodType);
                    if (prodOfTypeArr.length > 0) {

                        let prodDiv = document.createElement("div");
                        prodDiv.classList.add("row", "my-3");

                        let prodHeader = document.createElement("h2");
                        prodHeader.innerHTML = headerName;
                        prodHeader.classList.add("col-lg-4", "col-md-12");

                        let prodList = document.createElement("ul");
                        prodList.classList.add("list-unstyled", "col-lg-7", "col-md-12");

                        prodOfTypeArr.map(el => prodList.append(el));

                        prodDiv.append(prodHeader);prodDiv.append(prodList);
                        overarchingDiv.append(prodDiv);
                    }
                }

                let originalProdSeasonList = document.getElementsByClassName("list-unstyled");
                if (originalProdSeasonList.length > 0) {
                        originalProdSeasonList[0].innerHTML="";
                    }

                createNewHeaderAndList("Camp", "other camp dates");
                createNewHeaderAndList("Lunch", "lunch options");
                createNewHeaderAndList("Workshop", "workshops");
                createNewHeaderAndList("Unticketed Event", "events included with admission");
                createNewHeaderAndList("Admission Dailies", "drop-ins");
                createNewHeaderAndList("Ticketed Event", "upcoming event tickets");
            }


            if (document.getElementsByClassName("tn-event-detail").length > 0) {
                const eventProductType = document.getElementsByClassName("tn-event-detail")[0].getAttribute("data-tn-product-type-id");
                if (eventProductType=="13" || eventProductType=="14") { /* drop-ins and unticketed events */
                    let eventPerformanceName = document.getElementsByClassName("tn-event-detail__title")[0];
                    let eventProductionSeasonName = document.getElementsByClassName("tn-prod-season-header__title")[0];
                    eventProductionSeasonName.textContent = eventPerformanceName.textContent;
                    eventPerformanceName.style.display = "none";
                    
                    let ticketingSection = document.getElementById("tn-events-detail-best-available-form");
                    if (!!ticketingSection) {
                        ticketingSection.style.display = "none";
                    }

                    let metaDescription = document.querySelector("meta[name='description']").getAttribute("content"); /*using for time*/
                    if (metaDescription != null) {
                        let eventDateTime = document.getElementsByClassName("tn-event-detail__display-time")[0];
                        eventDateTime.textContent += " @ " + metaDescription;
                    }

                    if (document.getElementsByClassName("tn-prod-season-header__title-and-image-container").length > 0) {
                        let productionSeasonHeader = getFirstClassEl("tn-prod-season-header__title-and-image-container");
                        let eventDateTime = getFirstClassEl("tn-event-detail__display-time");
                        addPToEnd(productionSeasonHeader, eventDateTime.textContent);
                    }

                    let eventDetailMain = document.querySelector('main');
                    let backToCalDiv = document.createElement('div');
                        backToCalDiv.style.paddingTop = "20px";
                    let backToCalInner = document.createElement('a');
                        backToCalInner.href = "https://secure.pittsburghkids.org";
                        backToCalInner.innerHTML = "« all events";
                    backToCalDiv.append(backToCalInner);
                    eventDetailMain.prepend(backToCalDiv);
                    
                    
                    let admissionsURL = "https://secure.pittsburghkids.org/?kid=5";
                    const eventTitleDate = document.getElementById("tn-page-heading").innerText;
                    const eventDate = new Date(Date.parse(eventTitleDate.split(" ").slice(-5)));
                    if (!isNaN(eventDate.getTime())) {
                        const eventYear = eventDate.getFullYear();
                        const eventMonth = eventDate.getMonth()+1;
                        const eventDay = eventDate.getDate();
                        const dateString = eventYear + "-" + eventMonth + "-" + eventDay;
                        admissionsURL = "https://secure.pittsburghkids.org/?kid=5&span=day&startDate=" + dateString;
                    }

                    let eventDescriptionDiv = getFirstClassEl("tn-event-detail__main-container");
                    let admissionsBackLink = document.createElement('a');
                    admissionsBackLink.href = admissionsURL;
                    admissionsBackLink.innerHTML = "Included with Museum admission.";
                    eventDescriptionDiv.append(admissionsBackLink);



                    addStyle( `
                        .tn-prod-season-header__image-wrapper {
                            order: 3;
                            text-align: center;
                        }
                        .tn-prod-season-header__title-and-image-container {
                            display: flex;
                            flex-wrap: wrap;
                            flex-basis: 100%;
                        }
                        .tn-prod-season-header__title-and-image-container > * {
                            width: 100%;
                            padding: 0px 20px;
                        }
                        .tn-event-detail__additional-events-container, .tn-event-detail__display-time {
                            display: none;
                        }
                        .tn-events-detail-page {
                            max-width: 1000px;
                            margin: 0 auto;
                        }
                        
                        .tn-prod-season-header__title {
                            padding: 20px 0px;
                        }

                        .tn-prod-season-header__title-and-image-container > * {
                            padding: 0;
                        }

                        .tn-prod-season-header__image-wrapper {
                            display: block;
                        }

                        .tn-prod-season-header {
                            border-bottom: none;
                        }

                    `);

                    

                }
            }
            

            /* const keywordID = new URLSearchParams(document.location.search).get("kid");
            if (keywordID=="31") {  40-Person Spot Field Trip
                addStyle( `
                .events-calendar {
                    display: none;
                }
                .field-trip-info-40 {
                    display: inherit;
                }
                `);
            } */


            
            if (siteFlag=="cmp") {
                document.cookie = "site_theme=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;";

            }


            if (siteFlag=="mlab" || document.cookie.indexOf('site_theme=mlab') != -1) { /* MLab Template */
                
                
                function setMLabCookie() {
                    document.cookie = "site_theme=mlab; path=/;";
                }

                setMLabCookie();


                var favicon_32 = document.getElementById("favicon-32"),
                    favicon_192 = document.getElementById("favicon-192"),
                    favicon_apple = document.getElementById("favicon-apple"),
                    favicon_msapp = document.getElementById("favicon-msapp");
                
                favicon_32.setAttribute("href", "https://museumlab.org/wp-content/themes/museumlab/images/favicon.png");
                favicon_192.setAttribute("href", "https://museumlab.org/wp-content/themes/museumlab/images/favicon.png");
                favicon_apple.setAttribute("href", "https://museumlab.org/wp-content/themes/museumlab/images/favicon.png");
                favicon_msapp.setAttribute("content", "https://museumlab.org/wp-content/themes/museumlab/images/favicon.png");

               
                document.getElementById("masthead").innerHTML = `
                    <div class="mlab-container">
