                let menuBlockEl = document.getElementById("cmp-menu-block");

                function openMenuBlock() {
                    if (!!menuBlockEl) {
                        menuBlockEl.classList.add("show");
                        document.body.classList.add("noscroll");
                    }
                }

                function closeMenuBlock() {
                    if (!!menuBlockEl) {
                        menuBlockEl.classList.remove("show");
                        document.body.classList.remove("noscroll");
                    }
                }

                
